rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {

    match /projects/{document} {
      allow read: if true;
      allow write: if false;
    }

    match /masters/{document} {
      allow read: if true;
      allow write: if false;
    }

    match /bootcamp/{document} {
      allow read: if true;
      allow write: if false;
    }

  }
}
